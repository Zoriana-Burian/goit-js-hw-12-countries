<h1>{{name}}</h1>
<div>
    <ul>
        <li><span>Capital:</span>{{capital}}</li> 
        <li><span>Population:</span>{{population}}</li> 
        <li><span>Languages:</span>
        
         <ul>
             {{#each languages}}
             <li>{{name}}</li>
             {{/each}}
         </ul> 
        </li>
    </ul>
    <ul>
        <img src=" {{flag}} " alt=" {{name}} ">
    </ul>
</div>