 {{#each this}}<h1 class="name-c">{{name}}</h1>
<div class="country">
    <ul class="description">
        <li><span class = "text">Capital:</span>{{capital}}</li> 
        <li><span class="text">Population:</span>{{population}}</li> 
        <li><span class="text">Languages:</span>
        
         <ul>
             {{#each languages}}
             <li>{{name}}</li>
             {{/each}}
         </ul> 
        </li>
    </ul>
   
        <img class="flag" src="{{flag}}"  alt="{{name}}" >
    
</div>{{/each}}

